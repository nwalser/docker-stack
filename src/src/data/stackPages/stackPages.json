[
    {
        "id": "simple-mongo",
        "name": "Persistent MongoDB Stack",
        "description": "Simple MongoDB instance with public facing port and persistent storage.",
        "imageUrl": "https://www.nathaniel-walser.com/docker-compose-templates/mongo/header.svg",
        "exposedPorts": [
            {
                "name": "MongoDB Wire Protocol",
                "description": "Interface for communication with the mongo db.",
                "port": 27017,
                "webInterface": false
            },
            {
                "name": "MongoDB Express",
                "description": "MongoDB Express web interface",
                "port": 27017,
                "webInterface": true
            }
        ],
        "compose": {
            "version": "3.1",
            "services": [
                {
                    "id": "mongo",
                    "image": "mongo",
                    "tag": "6",
                    "ports": [
                        {
                            "container": 27017,
                            "host": 27017
                        }
                    ],
                    "volumeIds": [
                        {
                            "volumeId": "mongo-data",
                            "mountPath": "/data/db"
                        }
                    ]
                }
            ],
            "volumes": [
                {
                    "id": "mongo-data"
                }
            ],
            "comment": ""
        }
    },
    {
        "id": "persistent-mongo-with-express",
        "name": "Persistent MongoDB + Express Stack",
        "description": "Simple MongoDB instance with public facing port and a MongoDB Express Management interface.",
        "imageUrl": "https://www.nathaniel-walser.com/docker-compose-templates/mongo/header.svg",
        "compose": {
            "version": "3.1",
            "services": [
                {
                    "id": "mongo",
                    "image": "mongo",
                    "tag": "6",
                    "ports": [
                        {
                            "container": 27017,
                            "host": 27017
                        }
                    ],
                    "volumeIds": [
                        {
                            "volumeId": "mongo-data",
                            "mountPath": "/data/db"
                        }
                    ]
                },
                {
                    "id": "mongo-express",
                    "image": "mongo-express",
                    "tag": "latest",
                    "ports": [
                        {
                            "container": 8081,
                            "host": 8081
                        }
                    ],
                    "environmentVariables": [
                        {
                            "key": "ME_CONFIG_MONGODB_URL",
                            "value": "mongodb://mongo:27017/"
                        }
                    ]
                }
            ],
            "volumes": [
                {
                    "id": "mongo-data"
                }
            ]
        }
    }
]